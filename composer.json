{
    "name": "devrabiul/advanced-file-manager",
    "description": "An advanced file manager package for Laravel with support for cloud storage, flexible configurations, and a user-friendly API.",
    "keywords": ["laravel", "file manager", "storage", "s3", "cloud storage", "filesystem", "file upload"],
    "homepage": "https://github.com/devrabiul/advanced-file-manager",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Muhammad Rabiul",
            "email": "devrabiul@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Devrabiul\\AdvancedFileManager\\": "src/"
        },
        "files": [
            "src/utils/FileManagerTranslate.php"
        ]
    },
    "extra": {
        "laravel" :{
            "providers" : [
                "Devrabiul\\AdvancedFileManager\\AdvancedFileManagerServiceProvider"
            ],
            "aliases": {
                "AdvancedFileManager": "Devrabiul\\AdvancedFileManager\\Facades\\AdvancedFileManager"
            }
        }
    },
    "require": {
        "league/flysystem-aws-s3-v3": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
